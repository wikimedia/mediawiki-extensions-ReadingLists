{
	"root": true,
	"extends": [
		"wikimedia/client/common",
		"wikimedia/language/es2017",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"env": {
		"browser": true,
		"commonjs": true
	},
	"rules": {
		"compat/compat": "off"
	},
	"overrides": [
		{
			"files": [ "**/*.vue" ],
			"extends": [
				"wikimedia/vue3/common"
			],
			"rules": {
				"vue/component-name-in-template-casing": "off"
			}
		}
	]
}
