			"get": {
				"handler": {
					"class": "MediaWiki\\Extension\\ReadingLists\\Rest\\ListsHandler",
					"services": [
						"DBLoadBalancerFactory",
						"MainConfig",
						"CentralIdLookup"
					]
				}
			},
			"post": {
				"handler": {
					"class": "MediaWiki\\Extension\\ReadingLists\\Rest\\ListsCreateHandler",
